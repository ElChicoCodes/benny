{
	"core.input.keyboard" : {
		"patcher" : "core.input.keyboard",
		"type" : "note",
		"block_ui_patcher" : "blank.ui",
		"help_text" : "Midi keyboard input. Outputs notes played and the usual keyboard controllers. If auto assign is turned on then when a block is selected the keyboard notes are sent to that block instead, until the block is deselected. ",
		"exclusive" : 1,
		"max_polyphony" : 1,
		"autoload" : 1,
		"connections" : {
			"out" : {
				"midi" : [ "notes", "pressure", "bend", "bend-", "bend+", "modulation", "sustain" ]
			}
		},
		"groups" : [
			{ 
				"contains" : [0,1,2],
				"height" : 1
			}			
		],
		"parameters" : [
			{
				"name" : "pressure_rate_limit",
				"type" : "menu_i",
				"values" : ["off","128n","64n", "32nt", "32n", "16nt", "16n", "8nt", "8n", "4nt", "4n", "2nt", "2n", "1n"],
				"error_scale" : 0,
				"wrap" : 0,
				"default" : 0
			},{
				"name" : "keyboard_number",
				"type" : "menu_i",
				"values" : ["1","2","3", "4", "5", "6", "7", "8"],
				"error_scale" : 0,
				"wrap" : 0,
				"default" : 0
			},{
				"name" : "auto_assign",
				"type" : "menu_i",
				"values" : ["off","on"],
				"error_scale" : 0,
				"wrap" : 0,
				"default" : 0.25
			}
		]
	}
}