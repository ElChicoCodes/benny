, 			{
				"box" : 				{
					"code" : "Buffer midi_meters(\"midi_meters_buffer\");\r\nBuffer midi_scopes(\"midi_scopes_buffer\");\r\nBuffer midi_scopes_change(\"midi_scopes_change_buffer\");\r\n//inputs\r\n//1 = outputno 2 = note 3 = vel 4 = voiceno\r\nindex = in4;\r\nvflag, ch, held, p_min,p_max,v_min,v_max  = peek(midi_meters, index, 0, channels = 7);\r\n\r\nt_ind = (128*index + in1);\r\nnote = in2;\r\nm_ind = note + t_ind * 128;\r\n\r\nch = 0;\r\nif(in3 == 0){\r\n\t//remove from scopes\r\n\told = peek(midi_scopes, m_ind, 0);\r\n\tif(old != 0){\r\n\t\t//reduce held notecount for that output by 1.\r\n\t\tch=1;\r\n\t\theld -= (held>0);\r\n\t}\r\n\t//if it was any kind of min or max, recalculate those\r\n\tpoke(midi_scopes,0,m_ind,0);\r\n}else{\r\n\tvv=abs(in3);\r\n\t\r\n\t//add to scopes, if negative send a message to schedule wiping it in a moment\r\n\told = peek(midi_scopes,m_ind,0);\r\n\tif(old==0){//increase note held count\r\n\t\theld+=(in3>0);\r\n\t\tch = 1;\r\n\t\tpoke(midi_scopes_change, 1,t_ind,0);\r\n\t}\r\n\tpoke(midi_scopes,in3, m_ind, 0);\r\n\tif((vflag == -1)||(vflag == in1)){\r\n\t\tif(note < p_min){\r\n \t\t\tp_min = note;\r\n\t\t\tch=1;\r\n\t\t}\r\n\t\tif(note > p_max){\r\n\t\t\tp_max = note;\r\n\t\t\tch=1;\r\n\t\t}\r\n\t\tif(vv < v_min){\r\n\t\t\tv_min=vv;\r\n\t\t\tch=1;\r\n\t\t}\r\n\t\tif(vv > v_max){\r\n\t\t\tv_max=vv;\r\n\t\t\tch=1;\r\n\t\t}\r\n\t}\r\n}\r\nif(ch > 0){\r\n\t//poke(midi_meters, index, vflag, 0);\r\n\tpoke(midi_meters, ch, index, 1);\r\n\tpoke(midi_meters, held, index, 2);\r\n\tpoke(midi_meters, p_min, index, 3);\r\n\tpoke(midi_meters, p_max, index, 4);\r\n\tpoke(midi_meters, v_min, index, 5);\r\n\tpoke(midi_meters, v_max, index, 6);\r\n\tout2 = 1 + index;\r\n}else{\r\n\tout2 = 0;\r\n}\r\nif(in3 < 0){\r\n\tout1 = m_ind; //the output is used to turn off negative notes.\r\n}else{\r\n\tout1 = -1;\r\n}",
