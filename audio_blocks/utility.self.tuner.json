{
	"utility.self.tuner" : {
		"patcher" : "utility.self.tuner",
		"type" : "audio",
		"block_ui_patcher" : "blank.ui",
		"help_text" : "self-tuning midi to cv converter. connect your osc's output to the listen input. connect the cv output to the osc cv input. play notes.",
		"max_polyphony" : 0,
		"upsample" : 1,
		"connections" : {
			"in" : { 
				"audio" : [ "listen","fm" ],
				"midi" : [ "notes" ]
			},
			"out" : {
				"audio" : [ "cv" ]
			}
		},
		"groups" : [
			{ 
				"contains" : [0,1]
			}
		],
		"parameters" : [
			{
				"name" : "default_slope",
				"type" : "button",
				"values" : ["firstvoice", "default_slope", "default_slope"],
				"wrap" : 0,
				"default" : 0.2
			},
			{
				"name" : "calculate_slope",
				"type" : "button",
				"values" : ["firstvoice", "calculate_slope", "calculate_slope"],
				"wrap" : 0,
				"default" : 0.2
			}
		]
	}
