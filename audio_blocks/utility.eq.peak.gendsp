				"box" : 				{
					"code" : "//require \"sm_sallenkey_functions\";\r\nlinpeak(v0,cut,res,gain){\r\n\tParam drive(0.1);\r\n\tHistory ic1eq(0),ic2eq(0),g,k,a1,a2,idr(1),ocut(-1),ores(-1);\r\n\tif((cut!=ocut)||(ores!=res)){\r\n\t\tg = tan(PI * cut);\r\n\t\tk = 2 - 2 * res;\r\n\t\ta1 = 1 / (1 + g * (g+k));\r\n\t\ta2 = g*a1;\r\n\t\tocut=cut;\r\n\t\tores=res;\r\n\t\tidr=1/drive;\r\n\t}\r\n\tv1 = a1 * ic1eq + a2 * (v0 - ic2eq);\r\n\tv2 = ic2eq + g * v1;\r\n\tic1eq = 2 * v1 - ic1eq;\r\n\tic2eq = 2 * v2 - ic2eq;\r\n\tnotch = v0 - k*v1;\r\n\tpeak = v2 - notch;\r\n\t\r\n\treturn (gain>=1)? idr*asin(sin(clip(drive*peak * gain,-1.5,1.5)) + sin(clip(drive*v0,-1.5,1.5))) : notch * (1-gain) + gain * v0;\r\n\t//\treturn (gain>=1)? peak * gain + v0 : notch * (1-gain) + gain * v0;\r\n}\t\r\nBuffer prm(\"voice_parameter_buffer\");\r\nParam voice_offset(0);\r\nParam p(0);\r\n\r\nHistory c,c2, cut,res,drv,ll,lr,upda(0);\r\n\r\nif(upda<=0){\r\n\tupda = vectorsize+5;\r\n\tc = peek(prm,voice_offset, 0, channels=1);\r\n\tres = clip(peek(prm,1+voice_offset, 0, channels=1),0,0.99);\r\n\tgain = -36+72*peek(prm,2+voice_offset, 0, channels=1);\r\n\tpan = -2+ 4 * peek(prm,3+voice_offset, 0, channels=1);\r\n\tdrv = 0.05 + peek(prm,4+voice_offset,0,channels=1);\r\n\tc = pow(2,c)-1;\r\n\tc *= 8000/samplerate;\r\n\tc2 = c*(1+0.1*noise());\r\n\tp2 = clip(pan,-1,1);\r\n\tp3 = pan-p2;\r\n\tgain = dbtoa(gain)-1;\r\n\tgain = gain*(1-0.5*abs(p2));\r\n\tgg = (gain == -1) ? 0 : 1-(1/(1+gain));\r\n\tll = 1 + (1-p2)*gain -clip(p3,0,1)*gg;\r\n\tlr = 1 + (1+p2)*gain -clip(-p3,0,1)*gg;\r\n\tif(gain>-1){\r\n\t\tgs = 1 / sqrt(gain+1);\r\n\t\tll *= gs;\r\n\t\tlr *= gs;\r\n\t}\r\n}\r\nupda-=1;\r\ncut = slide(c,1000,1000);\r\ncut2 = slide(c2,1000,1000);\r\nr2 = slide(res,1000,1000);\r\nout1 = linpeak(in1,cut,r2,slide(ll,1000,1000),drive=0.1);\r\nout2 = linpeak(in2,cut2,r2,slide(lr,1000,1000),drive=0.1);",
